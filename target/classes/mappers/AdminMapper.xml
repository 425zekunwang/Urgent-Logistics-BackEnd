<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.urgentlogistics.mappers.AdminMapper">
    <select id="SelectByUserAndPassword" resultType="integer">
        select count(*) from admins where admin=#{username} and pwd=#{password};
    </select>
</mapper>